{
  "name": "actions-cache",
  "license": "MIT",
  "main": "lib/restore.js",
  "version": "1.2.3",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "jest",
    "all": "yarn build && yarn package",
    "release": "yarn all && git add -A && git commit -m=\"Release\" && git push origin master && gh release create \"$(npm version patch --git-tag-version=false)\" -t Release -d=false -n \"\" -p=false && git push --tags"
  },
  "dependencies": {
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "minio": "^7.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/minio": "^7.1.1",
    "@types/node": "^20.11.17",
    "@vercel/ncc": "0.38.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.1.0"
}
