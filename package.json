{
  "name": "actions-cache",
  "license": "MIT",
  "main": "lib/restore.js",
  "version": "1.2.0",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "jest",
    "all": "yarn build && yarn package",
    "release": "yarn all && git add -A && git commit -m=\"Release\" && git push origin master && gh release create \"$(npm version patch --git-tag-version=false)\" -t Release -d=false -n \"\" -p=false && git push --tags"
  },
  "dependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.2.3",
    "minio": "^7.0.18"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/minio": "^7.0.7",
    "@types/node": "^20.11.17",
    "@vercel/ncc": "0.34.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.2",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@4.1.0"
}
